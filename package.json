{
  "name": "slack-contentful",
  "version": "0.2.5",
  "description": "allows users to recieve push notifications on specified Contentful updates to a designated Slack channel",
  "main": "server.js",
  "scripts": {
    "test": "xo && nodeunit tests",
    "start": "node server.js"
  },
  "keywords": [
    "slack",
    "contentful",
    "node"
  ],
  "author": "Brandon Him",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "dotenv": "2.0.0",
    "express": "^4.13.3",
    "node-slack": "0.0.7",
    "nodeunit": "^0.9.1"
  },
  "devDependencies": {
    "xo": "^0.15.1"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "space": 4,
    "rules": {
      "new-cap": 0
    },
    "ignore": [
      "models/**"
    ]
  }
}
